{
    "name": "hiqdev/rdap",
    "type": "library",
    "description": "Registration Data Access Protocol (RDAP) - core domain entities implementation in PHP",
    "keywords": [
        "rdap",
        "php",
        "whois"
    ],
    "require": {
        "league/uri": "^5.3",
        "marc-mabe/php-enum": "^4.1",
        "sabre/vobject": "^4.2",
        "ext-mbstring": "*",
        "ext-intl": "*"
    },
    "suggest": {
        "league/fractal": "To transform objects to a JSON structure according to the RFC 7483"
    },
    "require-dev": {
        "hiqdev/hidev-php": "dev-master",
        "hiqdev/hidev-hiqdev": "dev-master",
        "league/fractal": "^0.18.0",
        "symfony/serializer": "^4.3.4",
        "phpunit/phpunit": "^8.3.4"
    },
    "autoload": {
        "psr-4": {
            "hiqdev\\rdap\\core\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "hiqdev\\rdap\\core\\tests\\": "tests"
        }
    }
}
